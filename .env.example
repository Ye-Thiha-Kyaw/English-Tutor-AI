# API Keys - 3 Key Rotation for Groq (load balancing & rate limit handling)
GROQ_API_KEY_1=your_first_groq_api_key_here
GROQ_API_KEY_2=your_second_groq_api_key_here
GROQ_API_KEY_3=your_third_groq_api_key_here

# Fallback single key (used if numbered keys not set)
GROQ_API_KEY=your_groq_api_key_here

# Database - Railway provides DATABASE_URL automatically for PostgreSQL
# For local development, leave empty to use SQLite
DATABASE_URL=

# Flask
SECRET_KEY=your_secret_key_here
DEBUG=True

# AI Model settings (Groq models: llama-3.3-70b-versatile, llama-3.1-8b-instant, mixtral-8x7b-32768)
MODEL_PROVIDER=groq
MODEL_NAME=llama-3.3-70b-versatile
MAX_TOKENS=1000
TEMPERATURE=0.7
